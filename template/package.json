{
  "name": "ProjectName",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean:project": "npx react-native-clean-project",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "reset": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:write": "eslint --debug app/ --fix",
    "prettier:write": "prettier --write '**/*.{js,ts,tsx}'",
    "type-check": "tsc -p tsconfig.json",
    "extract": "formatjs extract 'app/**/*.{js,jsx,ts,tsx}' --ignore='**/*.d.ts' --out-file extracted/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "compile": "formatjs compile extracted/en.json --out-file app/translations/en.json",
    "code": "yarn run prettier:write && yarn run type-check && yarn run lint:write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/geolocation": "^3.2.1",
    "@react-native-community/netinfo": "^11.3.2",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.4",
    "axios": "^1.6.8",
    "date-fns": "^3.6.0",
    "lodash": "^4.17.21",
    "react": "18.3.1",
    "react-hook-form": "^7.51.4",
    "react-intl": "^6.6.6",
    "react-native": "0.74.1",
    "react-native-confirmation-code-field": "^7.4.0",
    "react-native-date-picker": "^5.0.2",
    "react-native-device-info": "^10.13.2",
    "react-native-extra-dimensions-android": "^1.2.5",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-image-picker": "^7.1.2",
    "react-native-indicators": "^0.17.0",
    "react-native-modal": "^13.0.1",
    "react-native-permissions": "^4.1.5",
    "react-native-reanimated": "^3.11.0",
    "react-native-reanimated-carousel": "^3.5.1",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.2.0",
    "react-native-webview": "^13.10.2",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "use-debounce": "^10.0.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/eslint-parser": "^7.24.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-template-literals": "^7.24.1",
    "@babel/preset-env": "^7.24.5",
    "@babel/runtime": "^7.24.5",
    "@formatjs/cli": "^6.2.10",
    "@react-native/babel-preset": "^0.74.83",
    "@react-native/eslint-config": "^0.74.83",
    "@react-native/metro-config": "^0.74.83",
    "@react-native/typescript-config": "^0.74.83",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.1",
    "@types/react": "^18.3.2",
    "@types/react-native": "^0.72.8",
    "@types/react-native-indicators": "^0.16.6",
    "@types/react-redux": "^7.1.33",
    "@types/react-test-renderer": "^18.3.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-formatjs": "^10.5.14",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "git-format-staged": "^3.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.3.1",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn run prettier:write",
      "yarn run type-check",
      "yarn run lint"
    ]
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "yarn@3.6.4"
}
